<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:enterFadeDuration="@integer/default_animation_duration" android:exitFadeDuration="@integer/default_animation_duration">
    <item android:state_pressed="false">
        <shape>
            <gradient android:endColor="#93291E" android:startColor="#ED213A" />
            <corners android:radius="50dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#80ED213A" />
            <corners android:radius="50dp" />
        </shape>
    </item>
</selector>